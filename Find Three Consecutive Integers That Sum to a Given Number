class Solution {
public:
    vector<long long> sumOfThree(long long num) {
        if(num%3 != 0){
            return {};
        }
        vector<long long> output;
        output.push_back(num/3-1);
        output.push_back(num/3);
        output.push_back(num/3+1);
        return output;
        
    }
};
